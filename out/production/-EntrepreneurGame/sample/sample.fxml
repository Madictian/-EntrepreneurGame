                  <TextArea fx:id="InvestorThree" prefHeight="120.0" prefWidth="185.0" />
                  <TextArea fx:id="InvestorFour" prefHeight="120.0" prefWidth="185.0" />
                  <TextArea fx:id="InvestorFive" prefHeight="120.0" prefWidth="185.0" />
               </children>
            </HBox>
            <TextArea fx:id="PlayerPoints" layoutX="60.0" layoutY="505.0" prefHeight="117.0" prefWidth="151.0" />
